<?xml version='1.0' encoding='utf-8'?>
<root>
	<!--
		exportType	-	是否导出对应行，sq表示服务端不导出、cq表示客户端不导出
			id	-	热力活动ID
			time	-	活动时间
			type	-	条件类型
			factor	-	条件参数
			mintime	-	模式最小完成时间
			scale	-	折算比例
			expand	-	全区膨胀比例
			jump	-	提升热力转跳
			desc	-	热力说明
		-->
	<common id="1" time="20220720T000000,20220820T235959" type="2" factor="0" mintime="3" scale="1" expand="3" />
	<!--
		exportType	-	是否导出对应行，sq表示服务端不导出、cq表示客户端不导出
			reli_id	-	对应热力活动id
			reward_id	-	奖励id
			request_all	-	全区热力要求
			request_s	-	个人热力要求
			reward1	-	热力值奖励1
			reward2	-	热力值奖励2
			reward3	-	热力值奖励3
			reward4	-	热力值奖励4
			img_id	-	宝箱图片id
		-->
	<reward reli_id="1" reward_id="1" request_all="10000" request_s="100" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="2" request_all="20000" request_s="200" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="3" request_all="30000" request_s="300" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="4" request_all="40000" request_s="400" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="5" request_all="0" request_s="500" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="6" request_all="0" request_s="600" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="7" request_all="0" request_s="700" reward1="1315401,1" reward2="1315402,1" />
	<reward reli_id="1" reward_id="8" request_all="0" request_s="800" reward1="1315401,1" reward2="1315402,1" />
	<!--
		exportType	-	是否导出对应行，sq表示服务端不导出、cq表示客户端不导出
			rankBegin	-	该分段排名起点
			rankEnd	-	为该分段排名的重点
			items	-	奖励
			reli_id	-	对应热力活动id
		-->
	<ranking rankBegin="1" rankEnd="1" items="1315401,1;1315402,1" reli_id="1" />
	<ranking rankBegin="2" rankEnd="5" items="1315401,1;1315402,1" reli_id="1" />
	<ranking rankBegin="6" rankEnd="10" items="1315401,1;1315402,1" reli_id="1" />
	<!--
		exportType	-	是否导出对应行，sq表示服务端不导出、cq表示客户端不导出
			reli_id	-	对应热力活动id
			grow_id	-	增值id
			growtime	-	起始时间
			num	-	增长总值
		-->
	<grow reli_id="1" grow_id="1" growtime="20220720T000000,20220820T235959" num="5000" />
</root>